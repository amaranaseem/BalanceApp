{
  "expo": {
    "name": "BalanceApp",
    "slug": "BalanceApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.amara.balanceapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logo.jpg"
    },
    "plugins": [
      [
        "expo-build-properties"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8112f93a-7cf0-40f3-a4f1-80dc803bdeb5"
      }
    }
  }
}
